    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "howler": "^2.0.3",
    "jasmine-core": "^2.6.1",
    "jasmine-jquery": "^2.1.1",
    "jquery": "^3.2.1",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.3",
    "node-sass": "^4.5.2",
    "normalize-scss": "^6.0.0",
